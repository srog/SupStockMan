    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^3.2.0",
    "jsdom": "^9.10.0",
    "mocha": "^3.2.0",
    "mocha-eslint": "^3.0.1",
    "node-inspector": "^0.12.8",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
